		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"ionic-svelte": "^0.5.85",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass-embedded": "^1.79.3",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.4",
